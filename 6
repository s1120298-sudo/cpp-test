#include <iostream>
using namespace std;

int main() {
    float fahrenheit, celsius;

    提示使用者輸入華氏溫度
    cout << "20 ";
    cin >> fahrenheit;

    計算攝氏溫度
    celsius = (fahrenheit - 32) * 5.0 / 9.0;

     顯示結果
    cout << "華氏 " << fahrenheit << " 度等於 " << celsius << " 攝氏度。" << endl;

    return 0;
}